{
  "version": 2,
  "builds": [
    {
      "src": "docs-infernet_ml/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "docs-ritual_arweave/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/infernet_ml/(.*)",
      "dest": "docs-infernet_ml/$1"
    },
    {
      "src": "/ritual_arweave/(.*)",
      "dest": "docs-ritual_arweave/$1"
    }
  ]
}